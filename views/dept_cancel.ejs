<form action="/dept_cancel_reports" method="post">
    <label for="dept">Select Course</label>
    <select name="dept" id="dept">
        <% options.forEach(function(option) { %>
            <option value="<%= option %>"><%= option %></option>
        <% }); %>
    </select>
<button type="submit">search</button>
</form>

<% if (typeof data != 'undefined') { %>
    <% if (data == 'no data') { %>
      <br>
      <h2>User not Found</h2>
    <% } else { %>
      <h2>No of Canceled Admission in <span><%= dept %></span>:<%= data.length %></h2>
      <table border="2" style="border-collapse: collapse;">
        <tr>
            <th>Date</th>
          <th>Name</th>
          <th>Department</th>
          <th>User Id</th>
        </tr>
        <% data.forEach(function(item) { %>
          <tr>
            <td><%= item ? item.date : "no name" %></td>
            <td><%= item ? item.s_name : "no name" %></td>
            <td><%= item ? item.cname : "no name" %></td>
            <td><%= item ? item.uid : "no name" %></td>
          </tr>
        <% }); %>
      </table>
    <% } %>
  <% } else { %>
    <h2>Without data</h2>
  <% } %>
  